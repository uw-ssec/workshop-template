{
    "name": "Default",
    "image": "ghcr.io/prefix-dev/pixi:latest",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter",
                "ms-python.python",
                "ms-vsliveshare.vsliveshare",
                "DavidAnson.vscode-markdownlint",
                "jjjermiah.pixi-vscode",
                "GitHub.copilot"
            ],
            "settings": {
                "notebook.output.wordWrap": true
            }
        },
        "codespaces": {
            "openFiles": [
                "README.md"
            ]
        }
    }
}